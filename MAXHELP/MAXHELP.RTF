\par \pard\plain \s3\sb340\keepn \b\f5\fs28\cf7 \page {\cs75\fs16\up6 #{\footnote \pard\plain \s76 \b\f5\fs20 {\cs75\fs16\up6 #} QMTbasicscreen.237}}{\fs24  }{\cs75\fs16\up6 ${\footnote \pard\plain \s76 \b\f5\fs20 {\cs75\fs16\up6 $} QMT basic screen}}{\fs24  }
