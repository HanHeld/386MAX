\par \pard\plain \s2\sb362\sa43\keepn \b\f5\fs36\cf7 {\*\bkmkstart _Ref328147925}{\*\bkmkstart _Ref337121680}\page {\cs75\fs16\up6 #{\footnote \pard\plain \s76 \b\f5\fs20 {\cs75\fs16\up6 #} 386DISK.305}}{\fs24  }{\cs75\fs16\up6 ${\footnote \pard\plain \s76 
\b\f5\fs20 {\cs75\fs16\up6 $} 386DISK}}{\fs24  }{\cs75\fs16\up6 K{\footnote \pard\plain \s76 \b\f5\fs20 {\cs75\fs16\up6 K} 386DISK}}{\fs24  }{\cs75\fs16\up6 +{\footnote \pard\plain \s76 \b\f5\fs20 {\cs75\fs16\up6 +} SECTION14:0015}}{\fs24  }386DISK
